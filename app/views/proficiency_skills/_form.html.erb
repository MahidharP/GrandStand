<%= nested_form_for @proficiency_skill do |f| %>
<% if @proficiency_skill.errors.any? %>
<div id="error_explanation">
    <h2><%= pluralize(@proficiency_skill.errors.count, "error") %>
        prohibited this proficiency_skill from being saved:</h2>

    <ul>
        <% @proficiency_skill.errors.full_messages.each do |message| %>
        <li><%= message %></li>
        <% end %>
    </ul>
</div>
<% end %>

<div class="col-md-12">
    <div class="form-group">
        <%= f.label :name %><br>
        <%= f.text_field :name, class: "form-control" %>
    </div>
</div>

<div class="actions">
    <%= f.submit %>
</div>

<% end %>
